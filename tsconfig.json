{
    "compilerOptions": {
        "target": "ES6",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": ["src", "App.tsx","vite.config.ts"],
    "exclude": ["node_modules", "build"],
    "extends": "./tsconfig.paths.json"
}
